<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.boe.idm.project.mapper.primaryOracle.dev.SortBankMapper">

	<select id="getData" resultType="SortBankVO">
		SELECT productname,
		arrayproductspecname,
		cfproductspecname,
		cellproductspecname,
		arraymodeltype,
		cfmodeltype,
		cellmodeltype,
		tft,
		cf,
		pair,
		tft_bank_normal_n,
		tft_bank_normal_y,
		tft_bank_hold_n,
		tft_bank_hold_y,
		tft_bank_en_n,
		tft_bank_en_y,
		cf_bank_normal_n,
		cf_bank_normal_y,
		cf_bank_hold_n,
		cf_bank_hold_y,
		cf_bank_en_n,
		cf_bank_en_y
		FROM (SELECT
		B.NO,
		B.PRODUCTNAME,
		b.flag,
		B.ARRAYPRODUCTSPECNAME,
		B.CFPRODUCTSPECNAME,
		B.CELLPRODUCTSPECNAME,
		B.CELL_CFPRODUCTSPECNAME,
		B.ARRAYMODELTYPE,
		B.CFMODELTYPE,
		B.CELLMODELTYPE,
		A.HOURTIMEKEY,
		A.TFT,
		A.CF,
		A.PAIR,
		A.TFT_BANK_NORMAL_N,
		A.TFT_BANK_HOLD_N,
		A.TFT_BANK_EN_N,
		A.CF_BANK_NORMAL_N,
		A.CF_BANK_HOLD_N,
		A.CF_BANK_EN_N,
		A.TFT_BANK_NORMAL_Y,
		A.TFT_BANK_HOLD_Y,
		A.TFT_BANK_EN_Y,
		A.CF_BANK_NORMAL_Y,
		A.CF_BANK_HOLD_Y,
		A.CF_BANK_EN_Y FROM
		SC_SORT_BANE_VIEW A RIGHT JOIN SC_SORT_BANK_SPEC B ON (A.NO=B.NO
		AND
		HOURTIMEKEY=#{timekey}))
		where flag='æ˜¯'
		ORDER BY NO
	</select>

	<select id="getSumData" resultType="SortBankTftCfVO">
		select
		tft_bank_normal,tft_bank_hold,tft_bank_en,cf_bank_normal,cf_bank_hold,cf_bank_en
		from SC_SORT_BANE_VIEW1 where hourtimekey = #{timekey}
	</select>
</mapper>