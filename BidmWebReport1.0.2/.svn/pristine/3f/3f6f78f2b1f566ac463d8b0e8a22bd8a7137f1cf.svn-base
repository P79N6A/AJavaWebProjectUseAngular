package com.boe.idm.project.model.mybatis;

import org.apache.ibatis.type.Alias;

@Alias("ArrayMoveScrollVO")
public class ArrayMoveScrollVO {
	
	private String factory;
	private String month;
	private String datename;
	private String opercode;
	private String operdesc;
	private String productiontype;
	private String glsqty;
	private String lotqty;
	
	private int nowhour; // 这个是用在查询的时候，判断查询月份
	
	
	public int getNowhour() {
		return nowhour;
	}
	public void setNowhour(int nowhour) {
		this.nowhour = nowhour;
	}
	
	
	public String getFactory() {
		return factory;
	}
	public void setFactory(String factory) {
		this.factory = factory;
	}
	public String getMonth() {
		return month;
	}
	public void setMonth(String month) {
		this.month = month;
	}
	public String getDatename() {
		return datename;
	}
	public void setDatename(String datename) {
		this.datename = datename;
	}
	public String getOpercode() {
		return opercode;
	}
	public void setOpercode(String opercode) {
		this.opercode = opercode;
	}
	public String getOperdesc() {
		return operdesc;
	}
	public void setOperdesc(String operdesc) {
		this.operdesc = operdesc;
	}
	public String getProductiontype() {
		return productiontype;
	}
	public void setProductiontype(String productiontype) {
		this.productiontype = productiontype;
	}
	public String getGlsqty() {
		return glsqty;
	}
	public void setGlsqty(String glsqty) {
		this.glsqty = glsqty;
	}
	public String getLotqty() {
		return lotqty;
	}
	public void setLotqty(String lotqty) {
		this.lotqty = lotqty;
	}
	@Override
	public String toString() {
		return "ArrayMoveScrollVO [factory=" + factory + ", month=" + month + ", datename=" + datename + ", opercode="
				+ opercode + ", operdesc=" + operdesc + ", productiontype=" + productiontype + ", glsqty=" + glsqty
				+ ", lotqty=" + lotqty + "]";
	}
	public ArrayMoveScrollVO(String factory, String month, String datename, String opercode, String operdesc,
			String productiontype, String glsqty, String lotqty) {
		super();
		this.factory = factory;
		this.month = month;
		this.datename = datename;
		this.opercode = opercode;
		this.operdesc = operdesc;
		this.productiontype = productiontype;
		this.glsqty = glsqty;
		this.lotqty = lotqty;
	}
	public ArrayMoveScrollVO() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
	

}
