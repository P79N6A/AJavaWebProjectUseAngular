html {
    scroll-behavior: smooth;
}
body #myReTab .fa-close.fa {
    font-size: 12px;
    margin-top: 12px;
    // margin-left: 3px;
    display: none;
    position: absolute;
    right: 2px;
    color: #238ACB;
}
body #myReTab .fa-caret-down.fa {
    font-size:20px;
    // left: 4px
}

body #myReTab .fa[disabled] {
    display: none !important;
}
body #myReTab .fa.disabled {
    display: none !important;
}
body #classretab {
    float: left;
}
body #classretab .ui-tabview.ui-tabview-top > .ui-tabview-nav {
    border-bottom: 0px;
    height: 36px;
}
body #classretab .ui-tabview .ui-tabview-nav {
    white-space: nowrap;
    overflow-x: hidden;
    overflow-y: hidden;
}
body #classretab .ui-tabview .ui-tabview-nav > li .ui-tabview-title {
    margin: 0 10px;
    cursor: pointer;
    font-weight: bold;
}
body #classretab ::-webkit-scrollbar {
    height: 3px;
    width: 2px;
    background-color: #e2e1e1; /* or add it to the track */
}

body #classretab ::-webkit-scrollbar-thumb:horizontal{
    background: #969696; 
    border-radius: 10px;
    
}
body #classretab ::-webkit-scrollbar-thumb:hover {
    background-color: dodgerblue;
    
  }

body #classretab .ui-tabview .ui-tabview-nav li {
    display: inline-block;
    float: none;
}
body #classretab .ui-tabview .ui-tabview-nav > li > a {
    padding: 8px 0 0 0;
}
/* You can add global styles to this file, and also import other style files */
body #classretab .ui-tabview-panels  .ui-tabview-panel.ui-tabview-panel {
    padding: 0em 1em; 
}
body #classretab .ui-widget-content {
    background-color: #DBDBDB;
    border: 0px solid #DBDBDB;
}
body #classretab .ui-tabview.ui-tabview-top>.ui-tabview-nav li{
    top: 6px;
    height: 30px;
    // border-left: 1px solid lightgrey;
    // border-top: 1px solid lightgrey;
    // border-right: 1px solid lightgrey;
    padding: 0 5px 0 5px;
    border-width: 0px;
}
body #classretab .ui-tabview .ui-tabview-nav > li.ui-state-default a{ 
    font-size: 8px; 
    position: relative; 
}
body #classretab .ui-tabview .ui-tabview-nav > li.ui-state-default a:hover:after{
    content:attr(data-tooltip);
    display:none;
    padding:2px;
    color:#FFF;
    background:#333;
    position:absolute; top:17px; left:5px;
    white-space:nowrap;
    z-index: 10000;
    }
body #classretab .ui-tabview .ui-tabview-nav > li.ui-state-default a:hover:before{
    display:none;
    content:"";
    position:absolute; top:12px; left:5px;
    border-right:6px solid transparent; 
    border-bottom:6px solid #333; 
    border-left:6px solid transparent; 
    z-index: 10000; 
    }

