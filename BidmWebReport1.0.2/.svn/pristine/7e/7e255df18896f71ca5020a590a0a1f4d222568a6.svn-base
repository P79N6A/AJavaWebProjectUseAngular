<div class="ui-g" [ngClass]="{setBackground : true}">
  
  <!-- 设置标题 -->
  <div class="ui-g-12"><h3 [ngClass]="{setTitle : true}">公司级展示看板</h3></div>
  <!-- 设置月份 -->
  <div [ngClass]="{setTime : true}"><h3 >{{datecurrentstr}}</h3></div>

<!-- 四个大的柱状图 -->
  <div class="ui-g-12">
      <div class="ui-g-6"><div echarts [options]="chart_of_array" class="demo-chart" [ngClass]="{setEchartAr : true}" ></div></div>
      <div class="ui-g-6"><div echarts [options]="chart_of_cf" class="demo-chart" [ngClass]="{setEchartCf : true}"></div></div>
      <div class="ui-g-6"><div echarts [options]="chart_of_cell" class="demo-chart" [ngClass]="{setEchartCl : true}" ></div></div>
      <div class="ui-g-6"><div echarts [options]="chart_of_mdl" class="demo-chart" [ngClass]="{setEchartMdl : true}"></div> </div>
  </div>
</div>